{
  "name": "training-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run generate-api && env-cmd -f .env react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "generate-api": "openapi-generator-cli generate -g typescript-axios -o src/generated -i ../openApi.yaml",
    "build": "npm run generate-api && env-cmd -f .env.production react-scripts build",
    "prettier": "prettier src -w"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "@mui/x-data-grid": "^7.3.1",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "axios": "^1.6.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/react-dom": "^18.3.0",
    "env-cmd": "^10.1.0",
    "prettier": "^3.2.5",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
